class Solution {
public:
    bool isUgly(int n) {
        bool ugly=0;
        if(n>0)
        {
            if(n==1 )
        ugly=1;
        else
        {
        while(n%2==0)
            n=n/2;
        while(n%3==0)
            n=n/3;
        while(n%5==0)
            n=n/5;
            if(n==1)
                ugly= 1;
        }}
        
        return ugly;
    }
};
